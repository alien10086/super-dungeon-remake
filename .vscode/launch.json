{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Godot",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "/Applications/Godot_mono.app/Contents/MacOS/Godot",
            "args": ["--path", "${workspaceFolder}/super-dungeon-remake"],
            "cwd": "${workspaceFolder}/super-dungeon-remake",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}